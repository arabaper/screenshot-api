{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node",
          "config": {
        "installCommand": "bun install --legacy-peer-deps"
      }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.js"
      }
    ]
  }
  
